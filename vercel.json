{
    "version": 2,
    "regions": ["sin1"],
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "main.py"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "REDIS_URL": "@redis_url"
    }
} 